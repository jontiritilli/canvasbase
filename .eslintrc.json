{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-console": ["error", { "allow": ["warn"] }],
    "react/jsx-one-expression-per-line": "error",
    "react/jsx-indent": ["error", 2],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "quotes": ["error", "single"]
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  }
}